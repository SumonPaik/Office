<%- include('partial/header') %>

<div class="container">
    
    <h5>Basic Information</h5>
    <section id="common" style="display: flex; border: 1px solid #ccc; justify-content: space-evenly; margin-bottom: 30px;">
        <div id="basic" class="basic" style="border: 1px solid red; width: 33%;">
            
            <table>
                <tr>
                    <th>JOB NO.: </th>
                    <td><%= imports.jobno %></td>
                </tr>
                <tr>
                    <th>MBL No. : </th>
                    <td><%= imports.mblno %></td>
                </tr>
                <tr>
                    <th>Shipper : </th>
                    <td><%= imports.shipper %></td>
                </tr>
                <tr>
                    <th>Agent : </th>
                    <td><%= imports.agent %></td>
                </tr>
            </table>
        </div>
        <div id="basic-2" class="basic-2" style="border: 1px solid rgb(23, 105, 12); width: 33%;">
            
            <table>
                <tr>
                    <th>Date of Bord : </th>
                    <td><%= imports.dateonboard %></td>
                </tr>
                <tr>
                    <th>ETA : </th>
                    <td><%= imports.eta %></td>
                </tr>
                <tr>
                    <th>Origin : </th>
                    <td><%= imports.origin %></td>
                </tr>
                <tr>
                    <th>Con. Type : </th>
                    <td><%= imports.containertype %></td>
                </tr>
            </table>
        </div>
        <div id="basic-2" class="basic-2" style="border: 1px solid rgb(12, 60, 105); width: 33%;">            
            <table>
                <tr>
                    <th>Loading Port : </th>
                    <td><%= imports.portofloading %></td>
                </tr>
                <tr>
                    <th>Job Assign Date : </th>
                    <td><%= moment(imports.createdAt).format("DD MMM, YY")  %></td>
                </tr>
                <tr>
                    <th>Creator : </th>
                    <td><%= imports.origin %></td>
                </tr>
                <tr>
                    <th>Jon No. : </th>
                    <td><%= imports.containertype %></td>
                </tr>
            </table>
        </div>
    </section>

    <h5>House BL Information</h5>
    <section id="common" style="display: flex; border: 1px solid #ccc; justify-content: space-evenly;">
        <div id="basic" class="basic" style="border: 1px solid red; width: 100%;">
    
            <table style="width: 100%;">
                <tr>
                    <th>HBL No. : </th>
                    <th>Consignee : </th>
                    <th>Container No. : </th>
                    <th>Container Seal : </th>
                    <th>Carton/Quantity: </th>
                </tr>
                <tr>
                    <td>HBL 1</td>
                    <td>SHARK LTD</td>
                    <td>MEDU0123456</td>
                    <td>MSCABSCD</td>
                    <td>165CTN</td>

                </tr>                
            </table>
        </div>
    </section>
</div>

<%- include('partial/footer') %>