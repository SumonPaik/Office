
<%- include('partial/header') %>
    <div class="error" style="display: flex; flex-direction: column; justify-content: center; align-items: center;">
        <h2 style="color: #f32727; margin-top: 150px; font-size: 4rem; " class="error-header"><%= error.status %></h2>
        <h3 style="color: #1480d8; margin-top: 10px; font-size: 2rem; " class="error-message"><%= error.message %></h3>
        <h4 style="color: #1480d8; margin-top: 10px; font-size: 1.5rem; " class="error-message"><% if (error.status === 401) { %>
            <a href="/">Login</a>
        <% } %></h4>
        
            <% if (error.status === !401) { %>
            <h2 style="color: #1480d8; margin-top: 10px; font-size: 1.5rem; " class="error-message">
            <a href="/">Index</a>
            </h2>
            <% } %></h4>
        
    </div>
</body>
</html>